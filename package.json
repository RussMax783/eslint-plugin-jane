{
  "name": "eslint-plugin-jane",
  "description": "Jane's ESLint plugin and configurations",
  "version": "0.5.0",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "rules",
    "prettier.js"
  ],
  "scripts": {
    "format": "prettier \"*.js\" \"rules/*.js\" --write",
    "precommit": "lint-staged",
    "preversion": "npm t",
    "test": "run-s -s test:lint test:test",
    "test:lint": "eslint -c .eslintrc.json *.js rules",
    "test:test": "node test"
  },
  "homepage": "https://github.com/jane/eslint-plugin-jane#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jane/eslint-plugin-jane.git"
  },
  "bugs": "https://github.com/jane/eslint-plugin-jane/issues",
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "jane",
    "lint",
    "plugin"
  ],
  "dependencies": {
    "babel-eslint": "8.2.6",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.21.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.11.0",
    "eslint-plugin-unicorn": "5.0.0"
  },
  "devDependencies": {
    "eslint": "5.3.0",
    "eslint-plugin-self": "1.1.0",
    "flow-bin": "0.78.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.2",
    "run-s": "0.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "eslint": "^5.1.0"
  }
}
